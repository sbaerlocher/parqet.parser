# Parqet Parser Configuration
# Copy this file to .env and adjust the values as needed

# Directory paths
PARQET_DATA_DIR=data
PARQET_OUTPUT_DIR=output
PARQET_LOG_DIR=logs

# Config file path
PARQET_CONFIG_FILE=config.json

# Default timezone for processing
PARQET_TIMEZONE=Europe/Zurich

# Logging configuration
PARQET_LOG_LEVEL=INFO
PARQET_LOG_TO_CONSOLE=true

# CSV output options
PARQET_CSV_DELIMITER=,
PARQET_CSV_DECIMAL=,
